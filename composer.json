{
    "name": "larva/laravel-tencent-cloud",
    "description": "This is a Laravel expansion for the Tencent Cloud.",
    "keywords": [
        "Larva",
        "laravel",
        "tencent",
        "cloud"
    ],
    "type": "library",
    "license": "MIT",
    "require": {
        "ext-simplexml": "*",
        "ext-dom": "*",
        "ext-json": "*",
        "ext-libxml": "*",
        "illuminate/support": "^6.0 || ^7.0 || ^8.0 || ^9.0",
        "illuminate/bus": "^6.0 || ^7.0 || ^8.0 || ^9.0",
        "illuminate/http": "^7.0 || ^8.0 || ^9.0",
        "illuminate/queue": "^6.0 || ^7.0 || ^8.0 || ^9.0",
        "larva/tencentcloud-sdk-php": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Larva\\TencentCloud\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Larva\\TencentCloud\\TencentCloudServiceProvider"
            ],
            "aliases": {
                "TencentCloud": "Larva\\TencentCloud\\TencentCloud"
            }
        }
    },
    "prefer-stable": true
}
